{
  "kind": "collectionType",
  "collectionName": "tasks",
  "info": {
    "singularName": "task",
    "pluralName": "tasks",
    "displayName": "Task",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "taskTitle": {
      "type": "string"
    },
    "taskType": {
      "type": "enumeration",
      "enum": [
        "article",
        "video",
        "exercise"
      ]
    },
    "userFeedback": {
      "type": "boolean"
    },
    "accessCount": {
      "type": "integer"
    },
    "finishedReading": {
      "type": "boolean"
    },
    "week": {
      "type": "integer"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "to-do",
        "in-progress",
        "completed"
      ]
    },
    "taskTypes": {
      "type": "dynamiczone",
      "components": [
        "task-types.article",
        "task-types.exercise-task",
        "task-types.video-task"
      ],
      "max": 1
    }
  }
}
